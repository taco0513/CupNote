{
  "name": "cupnote",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 5173",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "lighthouse": "lhci autorun",
    "analyze": "ANALYZE=true npm run build",
    "crawler:start": "cd scripts/coffee-crawler && ts-node --project tsconfig.json src/crawler-manager.ts",
    "crawler:test": "cd scripts/coffee-crawler && ts-node --project tsconfig.json src/crawler-manager.ts",
    "crawler:list": "cd scripts/coffee-crawler && ts-node --project tsconfig.json src/crawler-manager.ts --list",
    "crawler:large-scale": "cd scripts/coffee-crawler && ts-node --project tsconfig.json src/large-scale-crawler.ts",
    "crawler:test-db": "cd scripts/coffee-crawler && ts-node --project tsconfig.json test-supabase-storage.ts",
    "crawler:migrate": "cd scripts/coffee-crawler && ts-node --project tsconfig.json run-migration.ts"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^15.4.5",
    "@supabase/supabase-js": "^2.57.0",
    "lucide-react": "^0.540.0",
    "next": "^15.1.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sharp": "^0.34.3",
    "tailwindcss": "^3.4.17",
    "task-master-ai": "^0.23.0",
    "typescript": "^5.8.3",
    "web-vitals": "^5.0.3"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^15.4.5",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cheerio": "^0.22.35",
    "@types/node": "^22.21.0",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "axios": "^1.11.0",
    "cheerio": "^1.1.2",
    "chrome-launcher": "^1.2.0",
    "dotenv": "^17.2.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "jsdom": "^26.1.0",
    "lighthouse": "^12.8.1",
    "prettier": "^3.6.2",
    "puppeteer": "^24.15.0",
    "ts-node": "^10.9.2",
    "vitest": "^3.2.4"
  },
  "description": "나만의 커피 여정을 기록하는 온라인 커피 일기",
  "keywords": [
    "coffee",
    "diary",
    "community",
    "tasting",
    "specialty coffee",
    "ocr",
    "machine learning"
  ],
  "author": "CupNote Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cupnote/cupnote"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}